# Maintainer: Erik Dubois <erik.dubois@gmail.com>
pkgname=bobo-grub-theme-vimix
_destname1="/boot"
_destname2="/etc"
_destname3="/etc"
_destname4="/etc"
pkgver=20.2.20
pkgrel=1
pkgdesc="Grub"
arch=('any')
url="https://github.com/PeterDauwe"
license=('GPL3')
makedepends=('git')
depends=()
conflicts=('bobo-grub-theme-vimix-git')
replaces=('bobo-grub-theme-vimix-git')
provides=("${pkgname}")
options=(!strip !emptydirs)
source=(${pkgname}::"git+${url}/${pkgname}")
sha256sums=('SKIP')
package() {
	install -dm755 ${pkgdir}${_destname1}
	cp -r ${srcdir}/${pkgname}${_destname1}/* ${pkgdir}${_destname1}
}
